{
  "capacityId": "{{ capacity_id }}",
  "client_id": "{{ client_id }}",
  "client_secret": "{{ client_secret }}",
  "tenant_id": "{{ tenant_id }}",
  "user_object_id": "{{ user_object_id }}",
  "workspaces": [
    {% for ws in workspaces %}
    {
      "name": "{{ ws.name }}"
      {% if ws.lakehouse %}, "lakehouse": "{{ ws.lakehouse }}" {% endif %}
      {% if ws.warehouse %}, "warehouse": "{{ ws.warehouse }}" {% endif %}
      {% if ws.notebook %}, "notebook": "{{ ws.notebook }}" {% endif %}
      {% if ws.pipelineStage %}, "pipelineStage": "{{ ws.pipelineStage }}" {% endif %}
    }{% if not loop.last %},{% endif %}
    {% endfor %}
  ]
}